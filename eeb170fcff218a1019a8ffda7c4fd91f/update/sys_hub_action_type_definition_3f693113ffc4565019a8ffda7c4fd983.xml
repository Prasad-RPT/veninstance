        <value>(function execute(inputs, outputs) {&#13;
// ... code ...&#13;
&#13;
var gr =new GlideRecord('x_rptp_smartinte_0_integration_values_validator_tables');&#13;
gr.addQuery('variant_id',req.variantId);&#13;
gr.addQuery('insuredperson',true);&#13;
gr.query();&#13;
if(gr.next()){&#13;
    if(gr.min_maturity_age){&#13;
        if( gr.min_maturity_age &gt; age + req.policyterm){&#13;
            outputs.code=400&#13;
            outputs.message="Minimum Maturity age for the product is "+gr.min_maturity_age;&#13;
            return;&#13;
        }&#13;
    }&#13;
    // max maturity age validation&#13;
     if(gr.max_maturity_age){&#13;
        if( age  + req.policyterm &gt; gr.max_maturity_age){&#13;
            outputs.code=400&#13;
            outputs.message="Maximum Maturity age  for the product is "+gr.max_maturity_age;&#13;
            return;&#13;
        }&#13;
    }&#13;
}&#13;
&#13;
&#13;
})(inputs, outputs);&#13;
